{
  "name": "@alexzzz/create-ec",
  "type": "module",
  "version": "0.2.4",
  "packageManager": "pnpm@7.1.2",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/alexzhang1030/ec#readme",
  "bugs": {
    "url": "https://github.com/alexzhang1030/ec/issues"
  },
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/ec.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "template"
  ],
  "bin": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp package.json --commit \"chore: release v%s\" --push --tag && pnpm -r publish --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@alexzzz/eslint-config": "^1.3.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "pnpm": "^7.1.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "^0.12.7"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
