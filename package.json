{
  "name": "@alexzzz/create-ec",
  "type": "module",
  "version": "0.2.5",
  "packageManager": "pnpm@7.3.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/alexzhang1030/ec#readme",
  "bugs": {
    "url": "https://github.com/alexzhang1030/ec/issues"
  },
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/ec.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "template"
  ],
  "bin": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp package.json --commit \"chore: release v%s\" --push --tag && pnpm -r publish --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@alexzzz/eslint-config": "1.3.0",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/node": "17.0.44",
    "bumpp": "8.2.1",
    "eslint": "8.18.0",
    "esno": "0.16.3",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "pnpm": "7.3.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "unplugin-auto-import": "0.8.8",
    "vitest": "0.15.2"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
